
@{
    ViewBag.Title = "AboutUs";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

<h2>About Us</h2>

@section LogIn{
    @if (Request.Cookies["user"] == null)
    {
        @Html.Partial("_LogIn")
    }
    else
    {
        @Html.Partial("_Welcom")
    }
}

@section Center{

    <img src="~/Image/Companie.jpg"/>

    @foreach (var item in ViewBag.AboutUs)
    {
        <div>@item</div><br />
    }
}


@section Menu{
    @Html.Partial("_Menu")
}


